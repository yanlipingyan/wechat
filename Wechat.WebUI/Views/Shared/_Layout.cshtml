    <!-- page specific plugin scripts -->

    <!--[if lte IE 8]>
		  <script src="~/Content/ace/assets/js/excanvas.min.js"></script>
	<![endif]-->

    <script src="~/Content/ace/assets/js/jquery-ui-1.10.3.custom.min.js"></script>
    <script src="~/Content/ace/assets/js/jquery.ui.touch-punch.min.js"></script>
    <script src="~/Content/ace/assets/js/jquery.slimscroll.min.js"></script>
    <script src="~/Content/ace/assets/js/jquery.easy-pie-chart.min.js"></script>
    <script src="~/Content/ace/assets/js/jquery.sparkline.min.js"></script>
    <script src="~/Content/ace/assets/js/flot/jquery.flot.min.js"></script>
    <script src="~/Content/ace/assets/js/flot/jquery.flot.pie.min.js"></script>
    <script src="~/Content/ace/assets/js/flot/jquery.flot.resize.min.js"></script>

    <!-- ace scripts -->
    <script src="~/Content/ace/assets/js/ace-elements.min.js"></script>
    <script src="~/Content/ace/assets/js/ace.min.js"></script>

    <!-- inline scripts related to this page -->
    @RenderSection("scripts", required: false)

    <div style="display: none">
        <script src='http://v7.cnzz.com/stat.php?id=155540&web_id=155540' language='JavaScript' charset='gb2312'></script>
    </div>
</body>
</html>
